{
  "name": "m-tartari.github.io",
  "version": "1.3.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "npm run dev",
    "start:prod": "npm run preview",
    "dev": "vite",
    "build": "npm run build:dev",
    "build:dev": "tsc && vite build",
    "build:prod": "tsc --project tsconfig.prod.json && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "npm run test -- --coverage",
    "test:ci-workflow": "npm ci --omit=dev && npm run build:prod && npm ci && npm run build && npm run lint && npm run test:coverage"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/oswald": "^5.2.8",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.0.0",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "analytics": "^0.8.19",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "react-scroll-parallax": "^3.5.0",
    "use-analytics": "^1.1.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/use-analytics": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-istanbul": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
